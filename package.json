{
  "name": "tactics-dex",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "prepare": "simple-git-hooks",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:unit": "vitest --project unit",
    "test:unit:run": "vitest run --project unit",
    "test:nuxt": "vitest --project nuxt",
    "pre-commit-check": "bun run lint:fix && bun run test:unit:run"
  },
  "dependencies": {
    "@nuxt/eslint": "1.13.0",
    "@nuxt/icon": "2.2.1",
    "@nuxt/image": "2.0.0",
    "@nuxt/test-utils": "3.23.0",
    "eslint": "^9.39.2",
    "nuxt": "^4.3.0",
    "vue": "^3.5.27",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^4.0.18",
    "@vue/test-utils": "^2.4.6",
    "simple-git-hooks": "^2.13.1",
    "vitest": "^4.0.18"
  },
  "simple-git-hooks": {
    "pre-commit": "bun run pre-commit-check",
    "pre-push": "bun run test"
  }
}
